{
    "name": "nextjs-ts",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "devTest": "cross-env NEXT_PUBLIC_ENV=test next dev -H 0.0.0.0",
        "dev": "cross-env NEXT_PUBLIC_ENV=test next dev",
        "dev:prod": "pnpm run build:prod && next start",
        "devTurbo": "cross-env NEXT_PUBLIC_ENV=test next dev --turbo",
        "build:test": "npm run prettier && npm run eslint-fix && cross-env NEXT_PUBLIC_ENV=test NODE_ENV=test next build",
        "build:prod": "npm run prettier && npm run eslint-fix && cross-env NEXT_PUBLIC_ENV=prod next build",
        "start:test": "cross-env NODE_ENV=test next start",
        "start": "next start",
        "lint": "next lint",
        "eslint-fix": "eslint --ext .jsx,.js,.ts,.tsx,.vue src/ --fix",
        "stylelint-fix": "stylelint src/**/*.vue --fix",
        "prettier": "prettier --write src/**/*.{ts,tsx}",
        "build:icon": "ts-node scripts/build_icon.js"
    },
